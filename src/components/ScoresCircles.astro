<style>
  @font-face {
    font-family: "Inter Bold";
    src: url("/fonts/Inter-Bold.woff");
    font-weight: 700;
    font-style: normal;
    font-display: swap;
  }
  .scores-circles-container {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 6.06rem 6%;
    /* box-shadow: 0px 0px 100px 0px #7bf55d; */
  }
  .scores-circle {
    width: 100%;
    height: 100%;
    flex-basis: 50%;
    display: flex;
    justify-content: center;
    align-items: center;
  }
  .score {
    position: absolute;
    font-family: "Inter Bold";
    font-size: 3.9375rem;
    color: #efffe2;
    text-align: center;
    text-shadow: 0px 0px 61px #53fb2a, 0px 0px 42px #e9e11f,
      0px 0px 12px #53fb2a;
  }
</style>
